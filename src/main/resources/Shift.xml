<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="martin_test.deeper.entities.Shift">
        <table name="SHIFT" schema="APP" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="startTime">
                <column name="START_TIME" precision="-1"/>
            </basic>
            <basic name="endTime">
                <column name="END_TIME" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
