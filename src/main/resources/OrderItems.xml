<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="martin_test.deeper.entities.OrderItems">
        <table name="ORDER_ITEMS" schema="APP" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="orderId">
                <column name="ORDER_ID" nullable="false" precision="-1"/>
            </basic>
            <basic name="menuItemId">
                <column name="MENU_ITEM_ID" nullable="false" precision="-1"/>
            </basic>
            <basic name="status">
                <column name="STATUS" precision="-1"/>
            </basic>
            <basic name="quantity">
                <column name="QUANTITY" precision="-1"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
