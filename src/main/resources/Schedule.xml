<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="martin_test.deeper.entities.Schedule">
        <table name="SCHEDULE" schema="APP" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="empId">
                <column name="EMP_ID" precision="-1"/>
            </basic>
            <basic name="mon">
                <column name="MON" nullable="false" precision="-1"/>
            </basic>
            <basic name="tue">
                <column name="TUE" nullable="false" precision="-1"/>
            </basic>
            <basic name="wed">
                <column name="WED" nullable="false" precision="-1"/>
            </basic>
            <basic name="thu">
                <column name="THU" nullable="false" precision="-1"/>
            </basic>
            <basic name="fri">
                <column name="FRI" nullable="false" precision="-1"/>
            </basic>
            <basic name="sat">
                <column name="SAT" nullable="false" precision="-1"/>
            </basic>
            <basic name="sun">
                <column name="SUN" nullable="false" precision="-1"/>
            </basic>
        </attributes>
    </entity>
    <entity class="martin_test.deeper.entities.Employee">
        <table name="EMPLOYEE" schema="APP" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="name">
                <column name="NAME" nullable="false" length="50"/>
            </basic>
            <basic name="deviceId">
                <column name="DEVICE_ID" nullable="false" length="50"/>
            </basic>
            <basic name="email">
                <column name="EMAIL" nullable="false" length="50"/>
            </basic>
            <basic name="phone">
                <column name="PHONE" nullable="false" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
