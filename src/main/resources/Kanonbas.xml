<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="martin_test.deeper.entities.Kanonbas">
        <table name="KANONBAS" schema="APP" catalog=""/>
        <attributes>
            <id name="id">
                <column name="ID" precision="-1"/>
            </id>
            <basic name="radius">
                <column name="RADIUS" nullable="false" precision="-1"/>
            </basic>
            <basic name="name">
                <column name="NAME" nullable="false" length="50"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
